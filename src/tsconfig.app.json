{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "outDir": "../out-tsc/app",
    "module": "es2015",
    "types": [],
    "baseUrl": "./",
    "paths": {
      "@shared/*":["app/shared/*"],
      "@core/*":["app/core/*"],
      "@modules/*":["app/modules/*"],
      "@env/*":["environments/*"],
      "@img/*":["assets/images/*"]
    }
  },
  "exclude": [
    "src/test.ts",
    "**/*.spec.ts"
  ]
}
